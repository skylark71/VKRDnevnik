                    android:text="@string/SetSub2"/>

                <ImageView
                    android:id="@+id/delTh2Sub"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="5dp"
                    android:src="@drawable/ic_delete" />
            </LinearLayout>

